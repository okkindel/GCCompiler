[ while.imp 
  12 23 34
  507 4379 0
]
DECLARE
	a; b; c; pom; pomj; pomk;
IN
	READ a;
	READ b;
	READ c;

	pom:=9;
	WHILE pom>0 DO
		pomj:=0;
		WHILE pomj<pom DO
			pomk:=0;
			WHILE pomk<pomj DO
				a := a + pomk;
				c := pomk * pomj;
				c := c + pom;
				b := b + c;
				pomk:=pomk+1;
			ENDWHILE
			pomj:=pomj+1;
		ENDWHILE
		pom:=pom-1;
	ENDWHILE

	WRITE a;
	WRITE b;
	WRITE c;
END