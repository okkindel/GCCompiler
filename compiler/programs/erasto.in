[ sito Eratostenesa ]
DECLARE
    j; sito(2:10000); n; 
IN
    n := 100000;
    FOR i FROM n DOWNTO 2 DO
        sito(i) := 1;
    ENDFOR
    FOR i FROM 2 TO n DO
        IF sito(i) != 0 THEN
            j := i + i;
            WHILE j <= n DO
                sito(j) := 0;
                j := j + i;
            ENDWHILE
            WRITE i;
        ENDIF
    ENDFOR
END
